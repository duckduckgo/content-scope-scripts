{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "FaviconFound",
  "required": ["favicons", "documentUrl"],
  "properties": {
    "favicons": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["rel", "href"],
        "title": "Favicon Attrs",
        "properties": {
          "href": {
            "type": "string"
          },
          "rel": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "description": "The MIME type from the link element's type attribute (e.g., image/svg+xml)"
          }
        }
      }
    },
    "documentUrl": {
      "type": "string"
    }
  }
}
