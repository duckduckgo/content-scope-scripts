{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "SurrogateInjected",
  "description": "Notification sent when a surrogate script is injected for a blocked tracker",
  "required": ["url", "blocked", "isSurrogate", "pageUrl"],
  "properties": {
    "url": {
      "type": "string",
      "description": "The URL of the blocked tracker resource"
    },
    "blocked": {
      "type": "boolean",
      "description": "Whether the request was blocked"
    },
    "reason": {
      "type": "string",
      "description": "The reason for blocking (e.g., 'surrogate', 'matched rule')"
    },
    "isSurrogate": {
      "type": "boolean",
      "description": "Whether a surrogate was injected"
    },
    "pageUrl": {
      "type": "string",
      "description": "The URL of the page where the tracker was detected"
    }
  }
}
