{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Favorites Refresh",
  "required": ["items"],
  "properties": {
    "items": {
      "type": "array",
      "items": {
        "oneOf": [
          {
            "type": "object",
            "required": ["kind"],
            "properties": {
              "kind": {
                "type": "string",
                "const": "favicons"
              }
            }
          }
        ]
      }
    }
  }
}
