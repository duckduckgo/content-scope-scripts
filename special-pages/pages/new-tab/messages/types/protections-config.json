{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Protections Config",
  "type": "object",
  "required": [
    "expansion",
    "feed"
  ],
  "properties": {
    "expansion": {
      "$ref": "expansion.json"
    },
    "feed": {
      "title": "Feed Type",
      "enum": [
        "privacy-stats",
        "activity"
      ]
    },
    "showBurnAnimation": {
      "description": "Boolean flag to explicitly enable or disable the burn animations",
      "type": "boolean"
    }
  }
}
