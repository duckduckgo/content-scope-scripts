{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Omnibar Config",
  "type": "object",
  "required": [
    "mode"
  ],
  "properties": {
    "mode": {
      "title": "Omnibar Mode",
      "type": "string",
      "enum": ["search", "ai"]
    },
    "enableAi": {
      "title": "Enable Duck.ai",
      "type": "boolean",
      "default": true
    },
    "showAiSetting": {
      "title": "Show AI Setting Button",
      "type": "boolean",
      "default": true
    }
  }
}
