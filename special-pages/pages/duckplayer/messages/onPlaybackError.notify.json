{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PlaybackError",
  "type": "object",
  "required": ["errorCode", "timestamp"],
  "properties": {
    "errorCode": {
      "type": "number",
      "description": "MediaError.code (1=MEDIA_ERR_ABORTED, 2=MEDIA_ERR_NETWORK, 3=MEDIA_ERR_DECODE, 4=MEDIA_ERR_SRC_NOT_SUPPORTED)"
    },
    "timestamp": {
      "type": "number",
      "description": "video.currentTime when error occurred"
    }
  }
}
