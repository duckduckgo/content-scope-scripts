{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PlaybackEvent",
  "type": "object",
  "required": ["eventType", "timestamp"],
  "properties": {
    "eventType": {
      "type": "string",
      "enum": ["start", "stalled", "resume", "error", "end"],
      "description": "Type of playback event"
    },
    "timestamp": {
      "type": "number",
      "description": "video.currentTime when event occurred"
    },
    "bufferAhead": {
      "type": "number",
      "description": "Seconds of buffer remaining (only for stalled)"
    },
    "stallDurationMs": {
      "type": "number",
      "description": "How long the stall lasted in milliseconds (only for resume)"
    },
    "errorCode": {
      "type": "number",
      "description": "MediaError.code (only for error)"
    }
  }
}
