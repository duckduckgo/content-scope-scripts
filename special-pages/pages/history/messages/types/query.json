{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "QueryKind",
  "oneOf": [
      {
        "type": "object",
        "title": "Search Term",
        "required": [
          "term"
        ],
        "properties": {
          "term": {
            "type": "string"
          }
        }
      },
      {
        "type": "object",
        "title": "Domain Filter",
        "required": [
          "domain"
        ],
        "properties": {
          "domain": {
            "type": "string"
          }
        }
      },
      {
        "type": "object",
        "title": "Range Filter",
        "required": [
          "range"
        ],
        "properties": {
          "range": {
            "$ref": "./range.json"
          }
        }
      }
  ]
}
