{
    "root": true,
    "plugins": ["promise", "@typescript-eslint", "eslint-plugin-n"],
    "parserOptions": {
      "ecmaVersion": "latest",
    },
    "rules": {
        "no-implicit-globals": "error",
        "typescript-eslint/ban-ts-comment": "off",
        "promise/prefer-await-to-then": "off",
    }
}
