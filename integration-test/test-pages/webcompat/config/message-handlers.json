{
  "unprotectedTemporary": [],
  "features": {
    "webCompat": {
      "exceptions": [],
      "state": "enabled",
      "settings": {
        "windowSizing": "enabled",
        "navigatorCredentials": "enabled",
        "safariObject": "enabled",
        "messageHandlers": {
          "state": "disabled",
          "handlerStrategies": {
            "reflect": ["trackerDetectedMessage", "printHandler", "specialPages"],
            "polyfill": ["*"],
            "undefined": ["jsHandler"]
          }
        },
        "domains": [
          {
            "domain": "localhost",
            "patchSettings": [
              {
                "op": "replace",
                "path": "/messageHandlers/state",
                "value": "enabled"
              }
            ]
          }
        ]
      }
    }
  }
}
